<h1>Users</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Apellido Paterno</th>
      <th>Apellido Materno</th>
      <th>Edad</th>
      <th>Peso</th>
      <th>Altura</th>
      <th>Talla</th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.nombre %></td>
        <td><%= user.apellidopaterno %></td>
        <td><%= user.apellidomaterno %></td>
        <td><%= user.edad %></td>
        <td><%= user.peso %></td>
        <td><%= user.altura %></td>
        <td><%= user.talla %></td>
        <td><%= user.email %></td>
        <td><%= link_to 'Mostrar', user %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
